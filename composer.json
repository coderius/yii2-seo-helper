{
    "name": "coderius/yii2-seo-helper",
    "description": "yii2 seo helper",
    "keywords": [
        "yii2",
        "yii2 modules",
        "yii2 extension",
        "seo",
        "tags",
        "optimize",
        "search-engine-optimizatio",
        "seo-meta",
        "seo-tools",
        "seo-optimization",
        "seo-tags",
        "helper",
        "yii2-seo",
        "yii2-helpers"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Serg Coderius",
            "email": "sunrise4fun@gmail.com",
            "role": "Developer"
        }
    ],
    
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "^2.0.0",
        "mockery/mockery": "^1.2"
   },
    "require-dev": {
        "phpunit/phpunit": "4.8.27|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "coderius\\yii2SeoHelper\\": "src"
        }
    },

    "config": {
        "fxp-asset": {
            "installer-paths": {
              "npm-asset-library": "vendor/npm",
              "bower-asset-library": "vendor/bower"
            }
        },    
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-pattern-skip-version": "(-patch)"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]

}
